<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.musicplayer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AcousticActivity"
            android:label="@string/acoustic"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".EnglishActivity"
            android:label="@string/english"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".RockActivity"
            android:label="@string/rock"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".EdmActivity"
            android:label="@string/edm"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>